<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >

<suite name="suite: single-duplicate-classes" verbose="2" parallel="tests">
    <test name="test: single-duplicate-classes">
        <classes>
            <class name="test.duplicate.FooTest">
                <methods>
                    <include name="foo1"/>
                    <include name="foo2"/>
                    <include name="foo3"/>
                    <include name="foo4"/>
                </methods>
            </class>
            <class name="test.duplicate.FooTest">
                <methods>
                    <include name="foo1"/>
                    <include name="foo2"/>
                    <include name="foo3"/>
                    <include name="foo4"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>


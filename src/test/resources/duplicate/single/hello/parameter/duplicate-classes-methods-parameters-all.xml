<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >

<suite name="suite: single-hello-duplicate-classes-methods-parameters-all" verbose="2">
    <parameter name="request_hello1" value="request_hello1-suite"/>
    <parameter name="response_hello1" value="response_hello1-suite"/>
    <parameter name="request_hello3" value="request_hello3-suite"/>
    <parameter name="response_hello3" value="response_hello3-suite"/>

    <test name="test: single-hello-duplicate-classes-methods-parameters-all">
        <classes>
            <class name="test.duplicate.HelloTest">
                <parameter name="request_hello1" value="request_hello1-class"/>
                <parameter name="response_hello1" value="response_hello1-class"/>
                <parameter name="request_hello3" value="request_hello3-class"/>
                <parameter name="response_hello3" value="response_hello3-class"/>
                <methods>
                    <parameter name="request_hello1" value="request_hello1-methods"/>
                    <parameter name="response_hello1" value="response_hello1-methods"/>
                    <parameter name="request_hello3" value="request_hello3-methods"/>
                    <parameter name="response_hello3" value="response_hello3-methods"/>
                    <include name="hello1">
                        <parameter name="request_hello1" value="request_hello1-include"/>
                        <parameter name="response_hello1" value="response_hello1-include"/>
                    </include>
                    <include name="hello3">
                        <parameter name="request_hello3" value="request_hello3-include"/>
                        <parameter name="response_hello3" value="response_hello3-include"/>
                    </include>
                    <include name="hello1"/>
                    <include name="hello2"/>
                    <include name="hello3"/>
                    <include name="hello4"/>
                    <include name="hello1"/>
                    <include name="hello2"/>
                </methods>
            </class>
            <class name="test.duplicate.HelloTest">
                <methods>
                    <include name="hello1"/>
                    <include name="hello2"/>
                    <include name="hello3"/>
                    <include name="hello4"/>
                    <include name="hello3"/>
                    <include name="hello4"/>
                </methods>
            </class>
            <parameter name="request_hello1" value="request_hello1-classes"/>
            <parameter name="response_hello1" value="response_hello1-classes"/>
            <parameter name="request_hello3" value="request_hello3-classes"/>
            <parameter name="response_hello3" value="response_hello3-classes"/>
        </classes>
    </test>

</suite>


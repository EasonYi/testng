        <classes>
            <class name="test.duplicate.HelloTest">
                <methods>
                    <include name="hello1"/>
                    <include name="hello2"/>
                    <include name="hello3"/>
                    <include name="hello4"/>
                </methods>
            </class>
            <class name="test.duplicate.HelloTest">
                <methods>
                    <include name="hello1"/>
                    <include name="hello2"/>
                    <include name="hello3"/>
                    <include name="hello4"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>

